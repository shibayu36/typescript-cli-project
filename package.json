{
  "name": "typescript-cli-project",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "bin": {
    "typescript-cli-project": "bin/typescript-cli-project.js"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci",
    "build": "webpack --silent",
    "exec": "npm run build && node dist/main.js",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check '*.js' 'src/**/*.ts'",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint . --fix",
    "lint-fix:prettier": "prettier --write '*.js' 'src/**/*.ts'",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit"
  },
  "author": "",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
